{
  "name": "oligus/jad",
  "description": "Json Api to Doctrine ORM",
  "keywords": [
    "json",
    "api",
    "doctrine",
    "orm"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "oligus",
      "email": "oli.gustafsson@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "symfony/http-foundation": "^4.1",
    "symfony/validator": "^4.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "doctrine/orm": "^2.6",
    "phpunit/dbunit": "^4.0",
    "spatie/phpunit-snapshot-assertions": "^1.3",
    "spatie/phpunit-watcher": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Jad\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jad\\Tests\\": "tests/src",
      "Jad\\Database\\": "tests/Database",
      "Jad\\E2E\\": "tests/e2e"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
