{
  "name": "oligus/jad",
  "description": "Json Api to Doctrine ORM",
  "keywords": [
    "json",
    "api",
    "doctrine",
    "orm"
  ],
  "type": "library",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "oligus",
      "email": "oli.gustafsson@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "tobscure/json-api": "^0.4.1",
    "symfony/http-foundation": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1",
    "doctrine/orm": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "Jad\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jad\\": "src/",
      "Jad\\Tests\\": "tests/src",
      "Jad\\Database\\": "tests/Database"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
